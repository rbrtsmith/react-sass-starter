{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": [ "jasmine" ],
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "comma-dangle": 0,
    "max-len": 0
  }
}